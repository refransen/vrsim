    border: 1px solid #CCCCCC!important;
    border-radius:10px;
    box-shadow: 5px 5px 20px 0px #666666;
    -webkit-box-shadow: 5px 5px 20px 0px #666666;
    -moz-box-shadow: 5px 5px 20px 0px #666666;
}

