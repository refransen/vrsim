/* Put a margin on the body if the dock is shown */
body.has_dock {margin-left:30px;}

/** For the dock itself */
#dock {width:30px;position:fixed;top:0px;left:0px;height:100%;background-color:#FFF;border-right:1px solid #000;z-index:11000;}
